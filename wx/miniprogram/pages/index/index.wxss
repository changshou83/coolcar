page {
  width: 100%;
  height: 100%;
}

#map {
  width: 100%;
  height: 100%;
}

/* 控件 */
.icon-container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;

  width: 87rpx;
  height: 136rpx;
  margin-top: 30rpx;

  opacity: .6;
}

.icon-container cover-image {
  width: 60rpx;
  height: 60rpx;
}
/* 底部按钮 */
.btn-container {
  display: flex;
  align-items: center;
  justify-content: center;

  width: 600rpx;
  height: 65rpx;
  margin-left: 75rpx;
  position: absolute;
  bottom: 7%;

  opacity: .8;
}
