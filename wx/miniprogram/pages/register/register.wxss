.license-placeholder {
  display: flex;
  align-items: center;
  justify-content: center;

  width: 600rpx;
  height: 450rpx;
  position: relative;
}

.license-placeholder-img {
  position: absolute;

  opacity: .2;
}

.stretch {
  width: 100%;
  height: 100%;
}

.check {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 0;
  right: 0;
}

.instruction {
  position: absolute;
  bottom: -74rpx;
}

.license-form {
  width: 600rpx;
  border-radius: 10rpx;
}

/* color ui */
.cuIcon-camerafill {
  font-size: 172rpx;
}

.cu-form-group:first-child {
  border: 1px transparent;
  border-radius: 16rpx 16rpx 0 0;
}
.cu-form-group:last-child {
  border: 1px transparent;
  border-radius: 0 0 16rpx 16rpx;
}

.cu-form-group .title {
  min-width: calc(5em + 30rpx);
}
